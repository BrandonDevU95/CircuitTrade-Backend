{
	"name": "circuittrade-backend",
	"version": "1.0.0",
	"description": "Backend de una plataforma de comercio electrónico B2B (Business-to-Business) está diseñada para facilitar las transacciones entre empresas.",
	"main": "index.js",
	"scripts": {
		"dev": "nodemon index.js",
		"start": "node index.js",
		"lint": "eslint",
		"format": "prettier --write .",
		"db:migrate": "sequelize db:migrate",
		"db:seed": "sequelize db:seed:all",
		"db:reset": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
		"test": "jest"
	},
	"keywords": [
		"circuittrade",
		"backend",
		"b2b",
		"api",
		"rest",
		"express",
		"sequelize",
		"mysql"
	],
	"author": "BrandonDevU95",
	"license": "MIT",
	"devDependencies": {
		"eslint": "9.7.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-prettier": "5.2.1",
		"jest": "29.7.0",
		"nodemon": "3.1.4",
		"prettier": "3.3.3",
		"sequelize-cli": "6.6.2",
		"supertest": "7.0.0"
	},
	"dependencies": {
		"@hapi/boom": "10.0.1",
		"bcrypt": "5.1.1",
		"cookie-parser": "^1.4.7",
		"cors": "2.8.5",
		"dotenv": "16.4.5",
		"express": "4.19.2",
		"helmet": "8.0.0",
		"joi": "17.13.3",
		"jsonwebtoken": "9.0.2",
		"mysql2": "3.10.3",
		"passport": "0.7.0",
		"passport-jwt": "4.0.1",
		"passport-local": "1.0.0",
		"sequelize": "6.37.3"
	},
	"engines": {
		"node": ">=18.0.0",
		"npm": ">=9.0.0"
	}
}
